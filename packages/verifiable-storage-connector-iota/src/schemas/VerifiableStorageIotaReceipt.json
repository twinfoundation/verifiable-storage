{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://schema.twindev.org/verifiable-storage/VerifiableStorageIotaReceipt",
	"description": "Receipt for the IOTA Verifiable Storage connector.",
	"type": "object",
	"properties": {
		"@context": {
			"type": "string",
			"const": "https://schema.twindev.org/verifiable-storage/",
			"description": "JSON-LD Context."
		},
		"type": {
			"type": "string",
			"const": "VerifiableStorageIotaReceipt",
			"description": "JSON-LD Type."
		},
		"epoch": {
			"type": "string",
			"description": "The epoch of the transaction."
		},
		"digest": {
			"type": "string",
			"description": "The digest of the transaction."
		}
	},
	"required": [
		"@context",
		"type",
		"epoch",
		"digest"
	],
	"additionalProperties": false
}
